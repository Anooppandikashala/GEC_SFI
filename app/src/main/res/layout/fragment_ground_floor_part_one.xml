<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ffffff"
    tools:context="anoop.myprojects.gec_sfi.GroundFloorPartOne">

    <!-- TODO: Update blank fragment layout -->
    <RelativeLayout
        android:layout_width="match_parent"
        android:background="@drawable/groundfloorpartone"
        android:layout_height="match_parent">

        <View
            android:id="@+id/ladiestoilet"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginLeft="32dp"
            android:layout_marginStart="32dp"
            android:layout_marginTop="96dp" />

        <View
            android:id="@+id/ecelab1"
            android:layout_width="50dp"
            android:layout_height="40dp"
            android:layout_alignTop="@+id/ladiestoilet"
            android:layout_toEndOf="@+id/ladiestoilet"
            android:layout_toRightOf="@+id/ladiestoilet" />

        <View
            android:id="@+id/ecelab2"
            android:layout_width="80dp"
            android:layout_height="40dp"
            android:layout_alignTop="@+id/ecelab1"
            android:layout_toEndOf="@+id/ecelab1"
            android:layout_toRightOf="@+id/ecelab1" />

        <View
            android:id="@+id/ecelab3"
            android:layout_width="80dp"
            android:layout_height="40dp"
            android:layout_alignTop="@+id/ecelab2"
            android:layout_toEndOf="@+id/ecelab2"
            android:layout_toRightOf="@+id/ecelab2" />



        <View
            android:id="@+id/itdept"
            android:layout_width="60dp"
            android:layout_height="55dp"
            android:layout_below="@+id/ecelab3"
            android:layout_marginLeft="14dp"
            android:layout_marginStart="14dp"
            android:layout_marginTop="29dp"
            android:layout_toEndOf="@+id/ecelab2"
            android:layout_toRightOf="@+id/ecelab2" />

        <View
            android:id="@+id/s1it"
            android:layout_width="60dp"
            android:layout_height="55dp"
            android:layout_alignLeft="@+id/itdept"
            android:layout_alignStart="@+id/itdept"
            android:layout_below="@+id/itdept" />

        <View
            android:id="@+id/itstaff1"
            android:layout_width="60dp"
            android:layout_height="55dp"
            android:layout_alignLeft="@+id/ladiestoilet"
            android:layout_alignStart="@+id/ladiestoilet"
            android:layout_below="@+id/itdept"
            android:layout_marginLeft="12dp"
            android:layout_marginStart="12dp" />

        <View
            android:id="@+id/s7it"
            android:layout_width="60dp"
            android:layout_height="55dp"
            android:layout_above="@+id/itstaff1"
            android:layout_alignLeft="@+id/itstaff1"
            android:layout_alignStart="@+id/itstaff1" />

        <View
            android:id="@+id/staffroomit2"
            android:layout_width="60dp"
            android:layout_height="55dp"
            android:layout_alignLeft="@+id/itstaff1"
            android:layout_alignStart="@+id/itstaff1"
            android:layout_below="@+id/itstaff1"
            android:layout_marginTop="28dp" />

        <View
            android:id="@+id/s5it"
            android:layout_width="60dp"
            android:layout_height="55dp"
            android:layout_alignLeft="@+id/staffroomit2"
            android:layout_alignStart="@+id/staffroomit2"
            android:layout_below="@+id/staffroomit2" />

        <View
            android:id="@+id/s3it"
            android:layout_width="60dp"
            android:layout_height="55dp"
            android:layout_alignLeft="@+id/s1it"
            android:layout_alignStart="@+id/s1it"
            android:layout_alignTop="@+id/staffroomit2" />



        <View
            android:id="@+id/staffroomit"
            android:layout_width="30dp"
            android:layout_height="55dp"
            android:layout_alignLeft="@+id/s3it"
            android:layout_alignStart="@+id/s3it"
            android:layout_below="@+id/s3it" />

        <View
            android:id="@+id/ithod"
            android:layout_width="30dp"
            android:layout_height="55dp"
            android:layout_below="@+id/s3it"
            android:layout_toEndOf="@+id/staffroomit"
            android:layout_toRightOf="@+id/staffroomit" />






    </RelativeLayout>


</FrameLayout>
